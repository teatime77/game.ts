                        }
                        ,
                        {
                            "className" : "ImageUI",
                            "imageFile" : "potato.png",
                            "size"      : [ 60, 60 ]
                        }
                        ,
                        {
                            "className" : "ImageUI",
                            "imageFile" : "suika.png",
                            "size"      : [ 60, 60 ]
                        }
